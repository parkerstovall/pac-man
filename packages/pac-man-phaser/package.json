{
  "name": "pac-man-phaser",
  "type": "module",
  "version": "1.0.8",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/assets"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && cp -r assets dist",
    "dev": "cp -r assets dist && tsc --watch",
    "lint": "eslint ./src --fix",
    "patch": "npm version patch --no-git-tag-version",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1"
  },
  "keywords": [
    "pacman",
    "map",
    "generator",
    "game"
  ],
  "author": "parkerstovall",
  "license": "MIT",
  "description": "Game of Pac-Man built using Phaser JS",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/parkerstovall/pac-man/issues"
  },
  "homepage": "https://github.com/parkerstovall/pac-man",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "printWidth": 80
  },
  "dependencies": {
    "pac-man-map-generator": "^1.0.4",
    "phaser": "^3.90.0"
  }
}
